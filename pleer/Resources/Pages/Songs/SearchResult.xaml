<Page x:Class="pleer.Resources.Pages.Songs.SearchResult"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:pleer.Resources.Pages.Songs"
          mc:Ignorable="d">

    <ScrollViewer VerticalAlignment="Top"
                  Style="{StaticResource ScrollViewer}">
        <StackPanel>
            <TextBlock Text="Результаты поиска" 
                       HorizontalAlignment="Left" Margin="5"
                       Style="{StaticResource MainInfoPanel}"/>
            <StackPanel Name="SongsList"/>
        </StackPanel>
    </ScrollViewer>
</Page>
